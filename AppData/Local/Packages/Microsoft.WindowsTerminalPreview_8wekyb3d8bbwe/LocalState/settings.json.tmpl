{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema-preview",
    "actions":
    [
      { "name": "Clear Marks",            "id": "User.clearAllMarks",          "command": "clearAllMarks"},
      { "name": "Toggle Mark Mode",       "id": "User.markMode",               "command": "markMode"},
      { "name": "Toggle Selection",       "id": "User.toggleBlockSelection",   "command": "toggleBlockSelection"},
      { "name": "First mark",             "id": "User.scrollToMark.First",     "command": { "action": "scrollToMark", "direction": "first" }},
      { "name": "Previous mark",          "id": "User.scrollToMark.Previous",  "command": { "action": "scrollToMark", "direction": "previous" }},
      { "name": "Next mark",              "id": "User.scrollToMark.Next",      "command": { "action": "scrollToMark", "direction": "next" }},
      { "name": "Last mark",              "id": "User.scrollToMark.Last",      "command": { "action": "scrollToMark", "direction": "last" }},
      { "name": "Mark Pink",              "id": "User.addMark.Magenta",        "command": { "action": "addMark", "color": "#FF00FF" }},
      { "name": "Mark Green",             "id": "User.addMark.Green",          "command": { "action": "addMark", "color": "#00FF00" }},
      { "name": "Mark Blue",              "id": "User.addMark.Blue",           "command": { "action": "addMark", "color": "#0000FF" }},
      { "name": "Shell completions",      "id": "User.sendInput.Complete",     "command": { "action": "sendInput", "input": "\u001b[24~b" }},
      { "name": "New Tab PowerShell",     "id": "User.newTab.PowerShell",      "command": { "action": "newTab", "profile": "Windows PowerShell", "tabColor": "#336699" } },
      { "name": "Split Pane PowerShell",  "id": "User.splitPane.WPowerShell",  "command": { "action": "splitPane", "profile": "Windows PowerShell", "split": "auto"} },
      { "name": "New Tab Ubuntu",         "id": "User.newTab.Ubuntu",          "command": { "action": "newTab", "profile": "Ubuntu", "tabColor": "#4b0631" } },
      { "name": "Split Pane Ubuntu",      "id": "User.splitPane.Ubuntu",       "command": { "action": "splitPane", "profile": "Ubuntu", "split": "auto"} },
      { "name": "New Tab Pwsh",           "id": "User.newTab.Pwsh",            "command": { "action": "newTab", "profile": "PwSh", "tabColor": "#012456" } },
      { "name": "Split Pane Pwsh",        "id": "User.splitPane.Pwsh",         "command": { "action": "splitPane", "profile": "PwSh", "split": "auto"} }
    ],
    "keybindings":[
      { "keys": "ctrl+f",            "id": "Terminal.FindText" },
      { "keys": "ctrl+shift+f",      "id": "Terminal.FindText" },
      { "keys": "ctrl+pgup",         "id": "User.scrollToMark.Previous" },
      { "keys": "ctrl+pgdn",         "id": "User.scrollToMark.Next" },
      { "keys": "ctrl+shift+pgup",   "id": "Terminal.ScrollToTop" },
      { "keys": "ctrl+shift+pgdn",   "id": "Terminal.ScrollToBottom" },
      { "keys": "pgdn",              "id": "Terminal.ScrollDownPage" },
      { "keys": "pgup",              "id": "Terminal.ScrollUpPage" },
      { "keys": "ctrl+1",            "id": "Terminal.SwitchToTab0" },
      { "keys": "ctrl+2",            "id": "Terminal.SwitchToTab1" },
      { "keys": "ctrl+3",            "id": "Terminal.SwitchToTab2" },
      { "keys": "ctrl+4",            "id": "Terminal.SwitchToTab3" },
      { "keys": "ctrl+5",            "id": "Terminal.SwitchToTab4" },
      { "keys": "ctrl+6",            "id": "Terminal.SwitchToTab5" },
      { "keys": "ctrl+7",            "id": "Terminal.SwitchToTab6" },
      { "keys": "ctrl+8",            "id": "Terminal.SwitchToTab7" },
      { "keys": "ctrl+9",            "id": "Terminal.SwitchToTab8" },
      { "keys": "ctrl+0",            "id": "Terminal.SwitchToTab9" },
      { "keys": "alt+shift+1",       "id": "Terminal.OpenNewTabProfile0" },
      { "keys": "alt+shift+2",       "id": "Terminal.OpenNewTabProfile1" },
      { "keys": "alt+shift+3",       "id": "Terminal.OpenNewTabProfile2" },
      { "keys": "alt+shift+4",       "id": "Terminal.OpenNewTabProfile3" },
      { "keys": "alt+shift+5",       "id": "Terminal.OpenNewTabProfile4" },
      { "keys": "alt+shift+6",       "id": "Terminal.OpenNewTabProfile5" },
      { "keys": "alt+shift+7",       "id": "Terminal.OpenNewTabProfile6" },
      { "keys": "alt+shift+8",       "id": "Terminal.OpenNewTabProfile7" },
      { "keys": "alt+shift+9",       "id": "Terminal.OpenNewTabProfile8" },
      { "keys": "alt+shift+0",       "id": "Terminal.OpenNewTabProfile9" },
      { "keys": "alt+d",             "id": "Terminal.DuplicatePaneAuto" },
      { "keys": "alt+shift+p",       "id": "User.newTab.Pwsh" },
      { "keys": "alt+p",             "id": "User.splitPane.Pwsh" },
      { "keys": "alt+shift+u",       "id": "User.newTab.Ubuntu" },
      { "keys": "alt+u",             "id": "User.splitPane.Ubuntu" },
      { "keys": "alt+shift+w",       "id": "User.newTab.PowerShell" },
      { "keys": "alt+w",             "id": "User.splitPane.PowerShell" }
    ],
